{
  "name": "checkout-nest",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "MIT-0",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "serve": "nest start",
    "serve:dev": "nest start --watch",
    "serve:debug": "nest start --debug --watch",
    "serve:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "exit 0",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:integration": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^9.3.10",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.3.10",
    "@nestjs/platform-express": "^9.4.3",
    "@nestjs/swagger": "^7.4.0",
    "@nestjs/terminus": "^10.2.3",
    "@opentelemetry/auto-instrumentations-node": "^0.56.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.57.1",
    "@opentelemetry/id-generator-aws-xray": "^1.2.2",
    "@opentelemetry/resource-detector-aws": "^1.11.0",
    "@opentelemetry/sdk-node": "^0.57.1",
    "@willsoto/nestjs-prometheus": "^5.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "nestjs-otel": "^6.0.0",
    "prom-client": "^14.2.0",
    "redis": "^4.7.0",
    "reflect-metadata": "^0.1.14",
    "request": "^2.88.2",
    "rimraf": "^6.0.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^9.4.2",
    "@types/express": "^4.17.21",
    "@types/jest": "28.1.8",
    "@types/node": "^22.10.7",
    "@types/supertest": "^2.0.11",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.2.2",
    "jest": "28.1.3",
    "prettier": "^3.1.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.3",
    "testcontainers": "^10.16.0",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
